  .wrapper
    .section__content--centered
      h2.section__title Références client
    .section
      .mod--large
        = link_to "#{image_tag "images/home/phildar.jpg", :class => "mod__img mod__img--rounded", :alt => "Phildar - Boutique en ligne de vente de laine"}", "https://www.phildar.fr", :target => "_blank"
      .mod--large
